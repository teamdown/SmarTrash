extends layout

block content
  style.
    #map {
      width: 100%;
      height: 500px;
    }
  .row
    .col-lg-12.grid-margin.stretch-card
      // weather card
      .card-body
        .weather-date-location
          #map
      // weather card ends
    //- .col-lg-5.grid-margin.stretch-card
      .card
        .card-body
          h2.card-title.text-primary.mb-5 Performance History
          .wrapper.d-flex.justify-content-between
            .side-left
              p.mb-2 The best performance
              p.display-3.mb-4.font-weight-light +45.2%
            .side-right
              small.text-muted 2017
          .wrapper.d-flex.justify-content-between
            .side-left
              p.mb-2 Worst performance
              p.display-3.mb-5.font-weight-light -35.3%
            .side-right
              small.text-muted 2015
          .wrapper
            .d-flex.justify-content-between
              p.mb-2 Sales
              p.mb-2.text-primary 88%
            .progress
              .progress-bar.bg-primary.progress-bar-striped.progress-bar-animated(role='progressbar', style='width: 88%', aria-valuenow='88', aria-valuemin='0', aria-valuemax='100')
          .wrapper.mt-4
            .d-flex.justify-content-between
              p.mb-2 Visits
              p.mb-2.text-success 56%
            .progress
              .progress-bar.bg-success.progress-bar-striped.progress-bar-animated(role='progressbar', style='width: 56%', aria-valuenow='56', aria-valuemin='0', aria-valuemax='100')
  .row
    .col-xl-3.col-lg-3.offset-lg-3.col-md-3.offset-md-3.col-sm-6.grid-margin.stretch-card
      .card.card-statistics
        .card-body
          .clearfix
            .float-left
              i.mdi.mdi-cube.text-danger.icon-lg
            .float-right
              p.mb-0.text-right Active SmarTrash Device
              .fluid-container
                h3.font-weight-medium.text-right.mb-0 1
    .col-xl-3.col-lg-3.col-md-3.col-sm-6.grid-margin.stretch-card
      .card.card-statistics
        .card-body
          .clearfix
            .float-left
              i.mdi.mdi-account-location.text-info.icon-lg
            .float-right
              p.mb-0.text-right Active Employees
              .fluid-container
                h3.font-weight-medium.text-right.mb-0 1
