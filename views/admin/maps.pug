  script.
    var map;
    function initMap() {
      var myCenter = new google.maps.LatLng(-18.930978, 47.528560);

      //Set marker for the center
      var center_marker = new google.maps.Marker({
      position: myCenter,
      animation: google.maps.Animation.BOUNCE
      });

      //InfoWindow
      var infowindow = new google.maps.InfoWindow({
        content: "Hi! Call me dude"
      });
      
      //Initialize map
      map = new google.maps.Map(document.getElementById('map'), {
        center: myCenter,
        zoom: 18,
        streetViewControl: true,
      });

      //Set event Listener
      google.maps.event.addListener(center_marker, 'click', function() {
        infowindow.open(map,center_marker);
      }); 
      center_marker.setMap(map);

      const smartrashes = JSON.parse('!{smartrashes}');
      const employees = JSON.parse('!{employees}');


      smartrashes.forEach(function(sc) {
        var marker = new google.maps.Marker({
          position: new google.maps.LatLng(sc.latitude, sc.longitude),
          icon: {
            url: 'data:image/png;base64,',
            scaledSize: new google.maps.Size(23, 33)
          },
          map: map,
          title: sc.location,
          animation: google.maps.Animation.DROP
        });
      });

      employees.forEach(function(sc) {
        var marker = new google.maps.Marker({
          position: new google.maps.LatLng(sc.latitude, sc.longitude),
          icon: {
            url: 'data:image/png;base64,',
            scaledSize: new google.maps.Size(50, 50)
          },
          map: map,
          title: sc.location,
          animation: google.maps.Animation.DROP
        });
      });
    }

  script(async defer src=`https://maps.googleapis.com/maps/api/js?key=${google_map_api_key}&callback=initMap`)
